{
  "name": "si-portal-glr-dev",
  "version": "2026-01-22.1",
  "description": "Mapping for si-portal-glr-dev Key Vault secrets to App Configuration keys (product-first with shared XML sender group).",
  "defaultBehavior": "reject-unmapped",
  "collisionPolicy": "keep-first",
  "createdAt": "2026-01-22T00:00:00Z",
  "createdBy": "ejernigan@sockeyeconsulting.com",
  "rules": [
    {
      "ruleId": "default-rest-client-id",
      "strategyType": "direct",
      "sourceSelector": "Default-IntacctRestApiClientId",
      "targetKey": "/crefoundations/shared/clientid",
      "priority": 100
    },
    {
      "ruleId": "default-rest-client-secret",
      "strategyType": "direct",
      "sourceSelector": "Default-IntacctRestApiClientSecret",
      "targetKey": "/crefoundations/shared/clientsecret",
      "priority": 99
    },
    {
      "ruleId": "imp-rest-client-id",
      "strategyType": "regex",
      "sourceSelector": "^(?<client>.+)-imp-IntacctRestApiClientId$",
      "targetKey": "/crefoundations/${client}/overrides/imp/clientid",
      "priority": 90
    },
    {
      "ruleId": "imp-rest-client-secret",
      "strategyType": "regex",
      "sourceSelector": "^(?<client>.+)-imp-IntacctRestApiClientSecret$",
      "targetKey": "/crefoundations/${client}/overrides/imp/clientsecret",
      "priority": 89
    },
    {
      "ruleId": "rest-client-id-default",
      "strategyType": "regex",
      "sourceSelector": "^(?<client>.+)-IntacctRestApiClientId$",
      "targetKey": "/crefoundations/shared/clientid",
      "priority": 85
    },
    {
      "ruleId": "rest-client-secret-default",
      "strategyType": "regex",
      "sourceSelector": "^(?<client>.+)-IntacctRestApiClientSecret$",
      "targetKey": "/crefoundations/shared/clientsecret",
      "priority": 84
    },
    {
      "ruleId": "imp-sender-id",
      "strategyType": "regex",
      "sourceSelector": "^(?<client>.+)-imp-SenderId$",
      "targetKey": "/crefoundations/${client}/overrides/imp/senderid",
      "priority": 88
    },
    {
      "ruleId": "imp-sender-password",
      "strategyType": "regex",
      "sourceSelector": "^(?<client>.+)-imp-SenderPassword$",
      "targetKey": "/crefoundations/${client}/overrides/imp/senderpassword",
      "priority": 87
    },
    {
      "ruleId": "sender-id",
      "strategyType": "regex",
      "sourceSelector": "^(?<client>.+)-SenderId$",
      "targetKey": "/crefoundations/${client}/senderid",
      "priority": 78
    },
    {
      "ruleId": "sender-password",
      "strategyType": "regex",
      "sourceSelector": "^(?<client>.+)-SenderPassword$",
      "targetKey": "/crefoundations/${client}/senderpassword",
      "priority": 77
    },
    {
      "ruleId": "access-enabled",
      "strategyType": "regex",
      "sourceSelector": "^(?<client>.+)-CREFoundations-AccessEnabled$",
      "targetKey": "/crefoundations/${client}/accessenabled",
      "priority": 70
    }
  ]
}
